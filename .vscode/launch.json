{
    "version": "0.2.0",
    "configurations": [
        {
            "env": {
                "DOTNET_ROOT": "C:/Program Files/dotnet/",
                "DOTNET_VERSION": "6.0.428"
            },
            "name": "Launch Godot",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/.vscode/find_godot.bat",
            "args": [
                "--path",
                "${workspaceFolder}"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
}